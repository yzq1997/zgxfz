}
.title{
    display: -webkit-box;             
    -webkit-box-orient: vertical;   
    font-family: PingFang-SC-Medium;
    color: rgba(26,26,26,1);
    -webkit-line-clamp: 1;            
    overflow: hidden;  
    font-size: 32rpx;
    margin: 8rpx 0;
}
.main{
    display: -webkit-box;             
    -webkit-box-orient: vertical;   
    font-family: PingFang;   
    -webkit-line-clamp: 2;            
    overflow: hidden;  
    font-size: 28rpx;
    margin: 8rpx 0;
}
